BANCO DE DADOS

BOOKS
CREATE TABLE books(id_book INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
livro_title VARCHAR(100) NOT NULL, image VARCHAR(255), book_author VARCHAR(100) NOT NULL, book_des VARCHAR(255) NOT NULL,
 book_gender VARCHAR(100) NOT NULL, livro_price INT NOT NULL
);

USERS
CREATE TABLE USERS (id_user INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
user_nome VARCHAR(150), password VARCHAR(60));
INSERT INTO USERS(USER_NOME, PASSWORD) VALUES ('ADMIN', '1234')
