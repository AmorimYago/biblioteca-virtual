BANCO DE DADOS

BOOKS
CREATE TABLE books(id INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
title VARCHAR(100) NOT NULL, image VARCHAR(255), author VARCHAR(100) NOT NULL, des VARCHAR(255) NOT NULL,
gender VARCHAR(100) NOT NULL, price DECIMAL(10, 2), isbn10 INT, isbn13 INT, editora VARCHAR(30)
);

USERS
CREATE TABLE cargo (id_user INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
username VARCHAR(150), password VARCHAR(60), useremail VARCHAR(100), date_nascimento DATE, telefone INT, cpf INT
);
