BANCO DE DADOS

BOOKS
CREATE TABLE books(id INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
title VARCHAR(100) NOT NULL, image VARCHAR(255), author VARCHAR(100) NOT NULL, des TEXT NOT NULL,
gender VARCHAR(100) NOT NULL, price DECIMAL(10, 2), isbn10 VARCHAR(255), isbn13 VARCHAR(255), editor VARCHAR(30)
);

USERS
CREATE TABLE Users (id_user INT NOT NULL  AUTO_INCREMENT PRIMARY KEY,
username VARCHAR(150), password VARCHAR(60), useremail VARCHAR(100), date_nascimento DATE, telefone INT, cpf INT
);
-Para criar o usuario do administrador-
INSERT INTO USERS(USERNAME, PASSWORD) VALUES ('ADMIN', '1234')
